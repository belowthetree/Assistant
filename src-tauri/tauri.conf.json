{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "assistant",
	"version": "0.1.0",
	"identifier": "com.assistant.app",
	"build": {
		"beforeDevCommand": "npm run dev",
		"devUrl": "http://localhost:5710",
		"beforeBuildCommand": "npm run build",
		"frontendDist": "../dist"
	},
	"app": {
		"security": {
			"csp": null
		},
		"macOSPrivateApi": true
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"resources": [
			"./resource"
		]
	}
}